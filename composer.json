    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "nyholm/psr7": "^1.1"
    },
    "suggest": {
        "symfony/http-foundation": "^4.1",
        "guzzlehttp/guzzle": "^5.0"
    }
}
